<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx" fx:id="sloganPane"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.sloganView"
            prefHeight="400.0" prefWidth="600.0">
    <TextField promptText="Slogan" fx:id="sloganText" layoutX="100" layoutY="100" />
    <Button text="Submit"     layoutX="100" layoutY="140"  onMouseClicked="#submitSlogan" />
</Pane>
